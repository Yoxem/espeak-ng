// Note:  ngK  matches "ng" if not followed by a vowel
// (D  will match on a digit and also if no digit is present. (special for tone
// language).

.group a
       a (X   a1
       a (L01 a4
       a (L02 a1
       a (_L03   a7
       a (L01_L03 a8
       a (L02_L03 a7
   /-%) a   a0
  /-%@) a  a0
// 輕聲可用
//       a (_@   a7
//       a (L01_@ a8
//       a (L02_@ a7
       ai (_	ai1
       au (_   au1
       ann (_ a~1
       ainn (_ ai~1
       aunn (_ au~1
       ann   a~
